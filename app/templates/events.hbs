{{#each event in model}}
  {{#link-to 'check-in'}}
    <div class="col-md-6">
      <div {{bind-attr class=':panel event.track.style'}}>
        <div class="panel-heading">
          <h2 class="panel-title">{{event.title}}</h2>
        </div>
        <div class="media">
          <div class="media-body">
            <div class="row">
              <div class="col-xs-9"><p><label>Speaker:</label> {{event.speaker}}</p>
                <p><label>Track:</label> {{event.track.name}}</p>
                <p><label>Location:</label> {{event.location.name}}</p></div>
              <div class="col-xs-3 event-time"><p>{{event.time}}</p><p>{{event.day}}</p></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  {{/link-to}}
{{/each}}